  "selector_strategy": {
    "based_on_screenshots": true,
    "primary_selectors": {
      "product_cards": "div[data-testid='alternative-card']",
      "product_names": "h3, h4, .product-name",
      "overall_ratings": "span[data-testid='star-rating-count']",
      "review_counts": "span:contains('(') and contains(')')",
      "pricing": "text:contains('Starting from:')",
      "rating_categories": "div[data-testid='review-summary-item']"
    },
    "fallback_selectors": {
      "product_cards": ".sb.card.padding-medium",
      "ratings": ".star-rating-label",
      "pricing": "text:contains('Per Month')"
    }
  }
